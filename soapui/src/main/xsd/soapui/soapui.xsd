        <xsd:attribute name="attachXmlBomb" type="xsd:boolean"></xsd:attribute>
        <xsd:attribute name="xmlAttachmentPrefix" type="xsd:string"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="MaliciousAttachment">
        <xsd:attribute name="filename" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="size" type="xsd:long"></xsd:attribute>
        <xsd:attribute name="contentType" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="enabled" type="xsd:boolean"></xsd:attribute>
        <xsd:attribute name="cached" type="xsd:boolean"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="MaliciousAttachmentElement">
        <xsd:sequence>
            <xsd:element name="replaceAttachment" type="tns:MaliciousAttachment"
                         maxOccurs="unbounded" minOccurs="0"></xsd:element>
            <xsd:element name="generateAttachment" type="tns:MaliciousAttachment"
                         maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="key" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="remove" type="xsd:boolean"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="MaliciousAttachmentSecurityCheck">
        <xsd:sequence>
            <xsd:element name="element" type="tns:MaliciousAttachmentElement"
                         minOccurs="0" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="requestTimeout" type="xsd:int"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="MaliciousAttachmentSecurityScan">
        <xsd:sequence>
            <xsd:element name="element" type="tns:MaliciousAttachmentElement"
                         minOccurs="0" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="requestTimeout" type="xsd:int"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="CheckedParameter">
        <xsd:attribute name="parameterName" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="checked" type="xsd:boolean"></xsd:attribute>
        <xsd:attribute name="xpath" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="type" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="label" type="xsd:string"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="CheckedParametersList">
        <xsd:sequence>
            <xsd:element name="parameters" type="tns:CheckedParameter"
                         maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SensitiveInformation">
        <xsd:complexContent>
            <xsd:extension base="tns:ModelItem">
                <xsd:sequence>
                    <xsd:element name="configuration" type="xsd:anySimpleType"></xsd:element>
                </xsd:sequence>
                <xsd:attribute name="level" type="xsd:string"></xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ExecutionStrategy">
        <xsd:sequence>
            <xsd:element name="strategy" minOccurs="1" maxOccurs="1"
                         type="tns:StrategyType">
            </xsd:element>
            <xsd:element name="delay" type="xsd:int" minOccurs="1"
                         maxOccurs="1"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="immutable" type="xsd:boolean"></xsd:attribute>
    </xsd:complexType>

    <xsd:simpleType name="StrategyType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ONE_BY_ONE"></xsd:enumeration>
            <xsd:enumeration value="ALL_AT_ONCE"></xsd:enumeration>
            <xsd:enumeration value="NO_STRATEGY"></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="InvalidSecurityCheck">
        <xsd:sequence>
            <xsd:element name="typesList" type="tns:SchemaTypeForSecurityCheck"
                         minOccurs="1" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InvalidSecurityScan">
        <xsd:sequence>
            <xsd:element name="typesList" type="tns:SchemaTypeForSecurityScan"
                         minOccurs="1" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SchemaTypeForSecurityCheck">
        <xsd:sequence>
            <xsd:element name="type" type="xsd:int"></xsd:element>
            <xsd:element name="value" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SchemaTypeForSecurityScan">
        <xsd:sequence>
            <xsd:element name="type" type="xsd:int"></xsd:element>
            <xsd:element name="value" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="XPathInjection">
        <xsd:sequence>
            <xsd:element name="xpathList" type="xsd:string"
                         minOccurs="1" maxOccurs="unbounded"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MalformedXml">
        <xsd:sequence>
            <xsd:element name="insertNewElement" type="xsd:boolean"></xsd:element>
            <xsd:element name="newElementValue" type="xsd:string"></xsd:element>
            <xsd:element name="leaveTagOpen" type="xsd:boolean"></xsd:element>
            <xsd:element name="changeTagName" type="xsd:boolean"></xsd:element>
            <xsd:element name="attributeMutation" type="tns:MalformedXmlAttribute">
            </xsd:element>
            <xsd:element name="insertInvalidCharacter" type="xsd:boolean"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MalformedXmlAttribute">
        <xsd:sequence>
            <xsd:element name="leaveAttributeOpen" type="xsd:boolean"></xsd:element>
            <xsd:element name="insertInvalidChars" type="xsd:boolean"></xsd:element>
            <xsd:element name="mutateAttributes" type="xsd:boolean"></xsd:element>
            <xsd:element name="newAttributeName" type="xsd:string"></xsd:element>
            <xsd:element name="newAttributeValue" type="xsd:string"></xsd:element>
            <xsd:element name="addNewAttribute" type="xsd:boolean"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="soapui-factories" type="tns:SoapUIFactories"></xsd:element>

    <xsd:complexType name="SoapUIFactory">
        <xsd:attribute name="id" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="factoryType" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="factoryClass" type="xsd:string"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="SoapUIFactories">
        <xsd:sequence>
            <xsd:element name="factory" type="tns:SoapUIFactory"
                         maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="searchPatterns" type="tns:SearchPatterns"></xsd:element>

    <xsd:complexType name="SearchPatterns">
        <xsd:sequence>
            <xsd:element name="Regex" type="tns:Regex" maxOccurs="unbounded"
                         minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Regex">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"></xsd:element>
            <xsd:element name="description" type="xsd:string"></xsd:element>
            <xsd:element name="message" type="xsd:string"></xsd:element>
            <xsd:element name="pattern" type="xsd:string" maxOccurs="unbounded"
                         minOccurs="1"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="FuzzerScan">
        <xsd:sequence>
            <xsd:element name="minimal" type="xsd:int"></xsd:element>
            <xsd:element name="maximal" type="xsd:int"></xsd:element>
            <xsd:element name="numberOfRequest" type="xsd:int"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="Environment">
        <xsd:sequence>
            <xsd:element name="service" type="tns:Service" maxOccurs="unbounded"
                         minOccurs="0">
            </xsd:element>
            <xsd:element name="property" type="tns:Property"
                         maxOccurs="unbounded" minOccurs="0">
            </xsd:element>
            <xsd:element name="databaseConnectionContainer" type="tns:DatabaseConnectionContainer"
                         maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string"></xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="EnvironmentEndpoint">
        <xsd:simpleContent>
            <xsd:extension base="tns:Endpoint">
                <xsd:attribute name="proxyHost" type="xsd:string"></xsd:attribute>
                <xsd:attribute name="proxyPort" type="xsd:string"></xsd:attribute>
                <xsd:attribute name="proxyUsername" type="xsd:string"></xsd:attribute>
                <xsd:attribute name="proxyPassword" type="xsd:string"></xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Service">
        <xsd:sequence>
            <xsd:element name="interfaceId" type="xsd:string"
                         maxOccurs="1" minOccurs="1"></xsd:element>
            <xsd:element name="endpoint" type="tns:EnvironmentEndpoint"
                         maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string"></xsd:attribute>
        <xsd:attribute name="type">
            <xsd:simpleType>
                <xsd:restriction base="tns:typeType">
                    <xsd:enumeration value="SOAP"></xsd:enumeration>
                    <xsd:enumeration value="REST"></xsd:enumeration>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:simpleType name="typeType">
        <xsd:restriction base="xsd:string"></xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="BreakPoint">
        <xsd:sequence>
            <xsd:element name="testStepId" type="xsd:string"></xsd:element>
            <xsd:element name="status" type="xsd:string"></xsd:element>
            <xsd:element name="properties" type="tns:BreakPointProperties"
                         maxOccurs="1" minOccurs="1"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BreakPointProperties">
        <xsd:sequence>
            <xsd:element name="conditionEnabled" type="xsd:boolean"></xsd:element>
            <xsd:element name="condition" type="tns:Script"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageContentElement">
        <xsd:sequence>
            <xsd:element name="xpath" type="xsd:string"></xsd:element>
            <xsd:element name="element" type="xsd:string"></xsd:element>
            <xsd:element name="type" type="xsd:string"></xsd:element>
            <xsd:element name="operator" type="xsd:string"></xsd:element>
            <xsd:element name="enabled" type="xsd:boolean"></xsd:element>
            <xsd:element name="expectedValue" type="xsd:string"></xsd:element>
            <xsd:element name="hasValue" type="xsd:boolean"></xsd:element>
            <xsd:element name="value" type="xsd:string"></xsd:element>
            <xsd:element name="numberType" type="xsd:boolean"></xsd:element>
            <xsd:element name="message" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageContentAssertion">
        <xsd:sequence>
            <xsd:element name="elements" type="tns:MessageContentElement"
                         maxOccurs="unbounded" minOccurs="0">
            </xsd:element>
            <xsd:element name="originalMessage" type="tns:CompressedString"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:attribute name="NewAttribute" type="xsd:string"></xsd:attribute>


    <xsd:complexType name="GroupAssertionList">
        <xsd:sequence>
            <xsd:element name="assertions" type="tns:AssertionEntry"
                         maxOccurs="unbounded" minOccurs="0">
            </xsd:element>
            <xsd:element name="operation" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AssertionTestStep">
        <xsd:sequence>
            <xsd:element name="entry" type="tns:AssertionEntry"
                         maxOccurs="unbounded" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AssertionEntry">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:string"></xsd:element>
            <xsd:element name="isGroup" type="xsd:boolean"></xsd:element>
            <xsd:element name="sourceId" type="xsd:string"></xsd:element>
            <xsd:element name="propertyName" type="xsd:string"></xsd:element>
            <xsd:element name="assertion" type="tns:TestAssertion"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>
