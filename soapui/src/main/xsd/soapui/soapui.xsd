                        <xsd:element name="name" type="xsd:string"/>
                        <xsd:element name="query" type="xsd:string"/>
                        <xsd:element name="match" type="xsd:string"/>
                        <xsd:element name="response" type="xsd:string"/>
                        <xsd:element name="disabled" type="xsd:boolean"/>
