              <action name="Set browser for opening readme in linux" id="4924" beanClass="com.install4j.runtime.beans.actions.control.SetVariableAction">
                <serializedBean>
                  <property name="script">
                    <object class="com.install4j.api.beans.ScriptProperty">
                      <property name="value" type="string">import com.install4j.runtime.installer.helper.InstallerUtil;
import com.install4j.runtime.installer.frontend.GUIHelper;

if (!(InstallerUtil.isUnattended() || InstallerUtil.isConsole() || InstallerUtil.isWindows() || InstallerUtil.isMacOS())) {
    for (String browser : new String[]{"xdg-open", "firefox", "opera", "konqueror", "epiphany", "mozilla", "netscape", "chromium-browser"}) {
        if (Runtime.getRuntime().exec(new String[]{"which", browser}).waitFor() == 0) {
            GUIHelper.setBrowserExecutable(browser);
            break;
        }
    }
}
return "";</property>
